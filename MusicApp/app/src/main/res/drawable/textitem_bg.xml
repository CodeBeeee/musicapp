<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 触摸模式下单击时的背景图片-->
    <item android:color="@color/colorGray2" android:state_pressed="true"></item>
    <!-- 非触摸模式下获得焦点并单击时的背景图片 -->
    <item android:color="@color/colorGray2" android:state_focused="true"></item>
    <!--默认背景-->
    <item android:color="@color/colorWhite"/>
</selector>