<!DOCTYPE mapper
        PUBLIC "mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.czf.web.mapper.AdminMapper">
    <select id="selectByName" resultType="com.czf.web.entity.Admin">
        select * from admin where adminusername = #{adminusername}
    </select>
</mapper>